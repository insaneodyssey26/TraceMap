      },
      {
        "command": "what-the-code.generateProjectReport",
        "title": "üìä Generate Project Report",
        "category": "What-The-Code",
        "icon": "$(graph)"
      },
      {
        "command": "what-the-code.openReportsFolder",
        "title": "üìÅ Open Reports Folder",
        "category": "What-The-Code",
        "icon": "$(folder-opened)"
      },
      {
        "command": "what-the-code.openReport",
        "title": "üìñ Open Report",
        "category": "What-The-Code",
        "icon": "$(go-to-file)"
      },
      {
        "command": "what-the-code.deleteReport",
        "title": "üóëÔ∏è Delete Report",
        "category": "What-The-Code",
        "icon": "$(trash)"
      },
      {
        "command": "what-the-code.openTeamLeaderboard",
        "title": "üèÜ Open Team Leaderboard",
        "category": "What-The-Code",
        "icon": "$(trophy)"
      },
      {
        "command": "what-the-code.togglePrivacyMode",
        "title": "üîí Toggle Privacy Mode",
        "category": "What-The-Code",
        "icon": "$(shield)"
      },
      {
        "command": "what-the-code.analyzeCurrentFile",
        "title": "üìä Analyze Current File Quality",
        "category": "What-The-Code",
        "icon": "$(pulse)"
      },
      {
        "command": "what-the-code.openCodeVisualizer",
        "title": "üå≥ Open Code Visualizer",
        "category": "What-The-Code",
        "icon": "$(type-hierarchy)"
      },
      {
        "command": "what-the-code.scanSecurity",
        "title": "üõ°Ô∏è Scan for Security Vulnerabilities",
        "category": "What-The-Code",
        "icon": "$(shield)"
      },
      {
        "command": "what-the-code.openSecurityIssue",
        "title": "Open Security Issue",
        "category": "What-The-Code",
        "icon": "$(go-to-file)"
      },
      {
        "command": "what-the-code.clearSecurityResults",
        "title": "Clear Security Results",
        "category": "What-The-Code",
        "icon": "$(clear-all)"
      },
      {
        "command": "what-the-code.generateSecurityReport",
        "title": "üìã Generate Security Report",
        "category": "What-The-Code",
        "icon": "$(file-text)"
      },
      {
        "command": "what-the-code.generateComplexityHeatmap",
        "title": "üìä Generate Complexity Heatmap",
        "category": "What-The-Code",
        "icon": "$(graph-line)"
      },
      {
        "command": "what-the-code.analyzeComplexity",
        "title": "üîç Analyze Complexity",
        "category": "What-The-Code",
        "icon": "$(sync)"
      },
      {
        "command": "what-the-code.clearComplexityResults",
        "title": "Clear Complexity Results",
        "category": "What-The-Code",
        "icon": "$(clear-all)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "what-the-code",
          "title": "What-The-Code",
          "icon": "media/search.svg"
        }
      ]
    },
    "views": {
      "what-the-code": [
        {
          "id": "what-the-code-main-actions",
          "name": "üöÄ Main Actions",
          "when": "true",
          "icon": "$(rocket)"
        },
        {
          "id": "what-the-code-results",
          "name": "üîç Search Results",
          "when": "true",
          "icon": "$(search-view-icon)"
        },
        {
          "id": "what-the-code-snapshots",
          "name": "üì∏ Code Snapshots",
          "when": "true",
          "icon": "$(save-all)"
        },
        {
          "id": "what-the-code-dead-code",
          "name": "üßπ Dead Code Analysis",
          "when": "true",
          "icon": "$(search-remove)"
        },
        {
          "id": "what-the-code-reports",
          "name": "üìÑ Quality Reports",
          "when": "true",
          "icon": "$(file-text)"
        },
        {
          "id": "what-the-code-visualizer",
          "name": "üå≥ Code Visualizer",
          "when": "true",
          "icon": "$(type-hierarchy)"
        },
        {
          "id": "what-the-code-security",
          "name": "üõ°Ô∏è Security Scanner",
          "when": "true",
          "icon": "$(shield)"
        },
        {
          "id": "what-the-code-complexity",
          "name": "üìä Complexity Analysis",
          "when": "true",
          "icon": "$(graph-line)"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "what-the-code.searchCode",
          "when": "view == what-the-code-main-actions",
          "group": "navigation@1",
          "icon": "$(search)"
        },
        {
          "command": "what-the-code.saveSnapshot",
          "when": "view == what-the-code-main-actions",
          "group": "navigation@2",
          "icon": "$(save)"
        },
        {
          "command": "what-the-code.testGemini",
          "when": "view == what-the-code-main-actions",
          "group": "navigation@3",
          "icon": "$(plug)"
        },
        {
          "command": "what-the-code.openSettings",
          "when": "view == what-the-code-main-actions",
          "group": "navigation@4",
          "icon": "$(settings-gear)"
        },
        {
          "command": "what-the-code.generateComplexityHeatmap",
          "when": "view == what-the-code-main-actions",
          "group": "navigation@5",
          "icon": "$(graph-line)"
        },
        {
          "command": "what-the-code.clearResults",
          "when": "view == what-the-code-results",
          "group": "navigation",
          "icon": "$(clear-all)"
        },
        {
          "command": "what-the-code.saveSnapshot",
          "when": "view == what-the-code-snapshots",
          "group": "navigation@1",
          "icon": "$(save)"
        },
        {
          "command": "what-the-code.clearAllSnapshots",
          "when": "view == what-the-code-snapshots",
          "group": "navigation@2",
          "icon": "$(clear-all)"
        },
        {
          "command": "what-the-code.findDeadCode",
          "when": "view == what-the-code-dead-code",
          "group": "navigation@1",
          "icon": "$(search-remove)"
        },
        {
          "command": "what-the-code.removeDeadCodeSafe",
          "when": "view == what-the-code-dead-code",
          "group": "navigation@2",
          "icon": "$(trash)"
        },
        {
          "command": "what-the-code.removeDeadCodeDryRun",
          "when": "view == what-the-code-dead-code",
          "group": "navigation@3",
          "icon": "$(eye)"
        },
        {
          "command": "what-the-code.removeDeadCodeInteractive",
          "when": "view == what-the-code-dead-code",
          "group": "navigation@4",
          "icon": "$(edit)"
        },
        {
          "command": "what-the-code.generateFileReport",
          "when": "view == what-the-code-reports",
          "group": "navigation@1",
          "icon": "$(file-text)"
        },
        {
          "command": "what-the-code.generateProjectReport",
          "when": "view == what-the-code-reports",
          "group": "navigation@2",
          "icon": "$(graph)"
        },
        {
          "command": "what-the-code.openReportsFolder",
          "when": "view == what-the-code-reports",
          "group": "navigation@3",
          "icon": "$(folder-opened)"
        },
        {
          "command": "what-the-code.openTeamLeaderboard",
          "when": "view == what-the-code-reports",
          "group": "navigation@4",
          "icon": "$(trophy)"
        },
        {
          "command": "what-the-code.generateComplexityHeatmap",
          "when": "view == what-the-code-reports",
          "group": "navigation@5",
          "icon": "$(graph-line)"
        },
        {
          "command": "what-the-code.openCodeVisualizer",
          "when": "view == what-the-code-visualizer",
          "group": "navigation@1",
          "icon": "$(type-hierarchy)"
        },
        {
          "command": "what-the-code.scanSecurity",
          "when": "view == what-the-code-security",
          "group": "navigation@1",
          "icon": "$(shield)"
        },
        {
          "command": "what-the-code.generateSecurityReport",
          "when": "view == what-the-code-security",
          "group": "navigation@2",
          "icon": "$(file-text)"
        },
        {
          "command": "what-the-code.clearSecurityResults",
          "when": "view == what-the-code-security",
          "group": "navigation@3",
          "icon": "$(clear-all)"
        },
        {
          "command": "what-the-code.analyzeComplexity",
          "when": "view == what-the-code-complexity",
          "group": "navigation@1",
          "icon": "$(sync)"
        },
        {
          "command": "what-the-code.generateComplexityHeatmap",
          "when": "view == what-the-code-complexity",
          "group": "navigation@2",
          "icon": "$(graph-line)"
        },
        {
          "command": "what-the-code.clearComplexityResults",
          "when": "view == what-the-code-complexity",
          "group": "navigation@3",
          "icon": "$(clear-all)"
        }
      ],
      "view/item/context": [
        {
          "command": "what-the-code.restoreSnapshot",
          "when": "view == what-the-code-snapshots && viewItem == snapshot",
          "group": "inline@1",
          "icon": "$(debug-restart)"
        },
        {
          "command": "what-the-code.deleteSnapshot",
          "when": "view == what-the-code-snapshots && viewItem == snapshot",
          "group": "inline@2",
          "icon": "$(trash)"
        },
        {
          "command": "what-the-code.openReport",
          "when": "view == what-the-code-reports && viewItem == report",
          "group": "inline@1",
          "icon": "$(go-to-file)"
        },
        {
          "command": "what-the-code.deleteReport",
          "when": "view == what-the-code-reports && viewItem == report",
          "group": "inline@2",
          "icon": "$(trash)"
        },
        {
          "command": "what-the-code.openSecurityIssue",
          "when": "view == what-the-code-security && viewItem == securityIssue",
          "group": "inline@1",
          "icon": "$(go-to-file)"
        }
      ]
    },
    "keybindings": [
      {
        "command": "what-the-code.searchCode",
        "key": "ctrl+shift+alt+k",
        "mac": "cmd+shift+alt+k"
      },
      {
        "command": "what-the-code.generateFileReport",
        "key": "ctrl+shift+alt+r",
        "mac": "cmd+shift+alt+r"
      },
      {
        "command": "what-the-code.openTeamLeaderboard",
        "key": "ctrl+shift+alt+l",
        "mac": "cmd+shift+alt+l"
      },
      {
        "command": "what-the-code.openCodeVisualizer",
        "key": "ctrl+shift+alt+v",
        "mac": "cmd+shift+alt+v"
      },
      {
        "command": "what-the-code.scanSecurity",
        "key": "ctrl+shift+alt+s",
        "mac": "cmd+shift+alt+s"
      },
      {
        "command": "what-the-code.generateComplexityHeatmap",
        "key": "ctrl+shift+alt+c",
        "mac": "cmd+shift+alt+c"
      }
    ],
    "configuration": {
      "title": "What-The-Code",
      "properties": {
        "whatTheCode.privacyMode": {
          "type": "boolean",
          "default": false,
          "description": "üîí Privacy Mode: Disable all AI features (Gemini API calls). All analysis runs locally.",
          "order": 0
        },
        "whatTheCode.geminiApiKey": {
          "type": "string",
          "default": "",
          "description": "Gemini API key (get from https://aistudio.google.com/app/apikey). Not used in Privacy Mode.",
          "order": 1
        },
        "whatTheCode.geminiModel": {
          "type": "string",
          "default": "gemini-2.5-flash",
          "description": "Gemini model to use for code analysis. Not used in Privacy Mode.",
          "enum": [
            "gemini-2.5-flash",
            "gemini-1.5-pro",
            "gemini-pro"
          ],
          "enumDescriptions": [
            "Fast and efficient",
            "Most capable",
            "Legacy model"
          ],
          "order": 2
        },
        "whatTheCode.maxFileSize": {
          "type": "number",
          "default": 50000,
          "description": "Maximum file size (in characters) to include in search"
        },
        "whatTheCode.includedExtensions": {
          "type": "array",
          "default": [
            ".js",
            ".ts",
            ".jsx",
            ".tsx",
            ".py",
            ".java",
            ".cs",
            ".cpp",
            ".c",
            ".h",
            ".go",
            ".rs",
            ".php",
            ".rb",
            ".vue",
            ".svelte"
          ],
          "description": "File extensions to include in code search"
        },
        "whatTheCode.deadCodeRemoval.createBackups": {
          "type": "boolean",
          "default": true,
          "description": "Create backup files before removing dead code"
        },
        "whatTheCode.deadCodeRemoval.confirmEachFile": {
          "type": "boolean",
          "default": false,
          "description": "Ask for confirmation before processing each file during removal"
        },
        "whatTheCode.deadCodeRemoval.onlyHighConfidence": {
          "type": "boolean",
          "default": true,
          "description": "Only remove high-confidence dead code items by default"
        },
        "whatTheCode.reports.openAfterGeneration": {
          "type": "boolean",
          "default": true,
          "description": "Automatically open HTML reports after generation"
        },
        "whatTheCode.reports.includeCharts": {
          "type": "boolean",
          "default": true,
          "description": "Include interactive charts in project reports"
        },
        "whatTheCode.reports.maxFilesInProjectReport": {
          "type": "number",
          "default": 20,
          "description": "Maximum number of files to analyze in project reports (for performance)"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src"
  },
  "devDependencies": {
    "@types/node": "20.x",
    "@types/vscode": "^1.101.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "axios": "^1.6.0"
  }
}
