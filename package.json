{
  "name": "what-the-code",
  "displayName": "What-The-Code",
  "icon": "media/logo.png",
  "description": "üîí Privacy-first code quality analyzer. Dead code detection, quality metrics, and refactoring suggestions ‚Äì all running locally. Optional AI search when you need it.",
  "version": "1.1.20",
  "publisher": "saheli56",
  "author": {
    "name": "Sk Masum Ali and Saheli Das"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/insaneodyssey26/what-the-code.git"
  },
  "bugs": {
    "url": "https://github.com/insaneodyssey26/what-the-code/issues"
  },
  "homepage": "https://github.com/insaneodyssey26/what-the-code#readme",
  "keywords": [
    "ai",
    "code-search",
    "dead-code",
    "analysis",
    "gemini",
    "snapshot",
    "refactor",
    "cleanup",
    "search",
    "assistant"
  ],
  "categories": [
    "Other",
    "Machine Learning",
    "Snippets"
  ],
  "engines": {
    "vscode": "^1.101.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
